		"reflect-metadata": "0.1.2",
		"rxjs": "5.0.0-beta.0",
		"systemjs": "0.19.6",
		"zone.js": "0.5.10"
	},
	"devDependencies": {
		"concurrently": "^1.0.0",
		"gulp": "^3.9.0",
		"gulp-sourcemaps": "^1.6.0",
		"gulp-typescript": "^2.10.0",
