    "concurrently": "^1.0.0",
    "gulp": "^3.9.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-typescript": "^2.10.0",
    "gulp-uglify": "^1.5.3",
    "gulp-watch": "^4.3.5",
    "lite-server": "^1.3.1",
    "typescript": "^1.7.3"
  }
